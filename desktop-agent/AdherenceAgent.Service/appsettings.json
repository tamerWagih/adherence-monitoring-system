{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.EventLog" ],
    "MinimumLevel": "Information"
  },
  "Agent": {
    "ApiEndpoint": "http://10.20.13.82/api",
    "SyncIntervalSeconds": 60,
    "BatchSize": 100,
    "IdleThresholdMinutes": 5,
    "IdleThresholdSeconds": 0,
    "WindowCheckIntervalSeconds": 5,
    "IdleCheckIntervalSeconds": 30,
    "MaxRetryAttempts": 3,
    "MaxBufferSize": 10000,
    "AgentVersion": "0.1.0",
    "EnableEventLog": false
  }
}
